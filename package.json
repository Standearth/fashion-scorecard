    "archieml": "^0.4.2",
    "d3": "^7.0.1",
    "d3-dsv": "^3.0.1",
    "eslint": "^7.22.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-svelte3": "^3.2.0",
    "font-awesome": "^4.7.0",
    "fontawesome-svelte": "^2.0.1",
    "framework7": "^6.3.0",
